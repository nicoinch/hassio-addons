{
  "name": "Nest Wifi",
  "version": "0.0.27",
  "boot": "auto",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "slug": "nest_wifi",
  "description": "Provide nest wifi sensor data",
